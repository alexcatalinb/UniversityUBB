@model IEnumerable<UniversityUBB.Models.SchoolViewModels.EnrollmentDateGroup>

@{
    ViewData["Title"] = "Student Body Statistics";
}

<h2>Situatie inscriere cursuri</h2>

<table>
    <tr>
        <th>
            Data insriere
        </th>
        <th>
            Numar studenti inscrisi
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.EnrollmentDate)
            </td>
            <td>
                @item.StudentCount
            </td>
        </tr>
    }
</table>